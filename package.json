{
	"name": "@corioders/projects-manager",
	"version": "1.0.0",
	"description": "Corioders' projects management tool",
	"repository": "https://github.com/corioders-templates/projects-manager.git",
	"author": "corioders",
	"license": "CC0-1.0",
	"scripts": {
		"postinstall": "husky install",
		"clean": "rm -rf out",
		"watch": "tsc --watch",
		"build": "yarn run clean && tsc && yarn run lint && tsc-alias -p tsconfig.json && yarn run makeExecutable",
		"lint": "eslint .",
		"release": "yarn run build && yarn run movePackageJson && cd out && npm publish --access public",
		"makeExecutable": "chmod +x ./out/bin/corioders.js",
		"movePackageJson": "cat ./package.json | json -e 'delete this.scripts; delete this.devDependencies' > ./out/package.json"
	},
	"bin": {
		"corioders": "./out/bin/corioders.js"
	},
	"devDependencies": {
		"@babel/eslint-parser": "^7.13.14",
		"@commitlint/cli": "^12.1.1",
		"@trivago/prettier-plugin-sort-imports": "^2.0.2",
		"@types/graceful-fs": "^4.1.5",
		"@types/inquirer": "^7.3.1",
		"@types/node": "^14.14.41",
		"@typescript-eslint/eslint-plugin": "^4.21.0",
		"@typescript-eslint/parser": "^4.21.0",
		"commitlint-config-jira": "^1.5.1",
		"commitlint-plugin-jira-rules": "^1.5.1",
		"eslint": "^7.23.0",
		"husky": "^6.0.0",
		"json": "^10.0.0",
		"prettier": "^2.2.1",
		"tsc-alias": "^1.2.9",
		"typescript": "^4.2.4"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"base64url": "^3.0.1",
		"commander": "^7.2.0",
		"graceful-fs": "^4.2.6",
		"inquirer": "^8.0.0",
		"simple-git": "^2.37.0",
		"tapable": "^2.2.0"
	}
}
