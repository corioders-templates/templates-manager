{
	"name": "@corioders/cli",
	"version": "1.0.0",
	"description": "Cli made for creating projects with our templates",
	"repository": "https://github.com/corioders-templates/cli-next.git",
	"author": "corioders",
	"license": "CC0-1.0",
	"scripts": {
		"postinstall": "husky install",
		"clean": "rm -rf out",
		"watch": "tsc --watch tsconfig.json",
		"build": "yarn run clean && tsc && yarn run lint && tsc-alias -p tsconfig.json",
		"lint": "eslint .",
		"release": "yarn run build && yarn run movePackageJson && cd out && npm publish --access public",
		"movePackageJson": "cat ./package.json | json -e 'delete this.scripts; delete this.devDependencies' > ./out/package.json"
	},
	"devDependencies": {
		"@babel/eslint-parser": "^7.13.14",
		"@commitlint/cli": "^12.1.1",
		"@trivago/prettier-plugin-sort-imports": "^2.0.2",
		"@typescript-eslint/eslint-plugin": "^4.21.0",
		"@typescript-eslint/parser": "^4.21.0",
		"commitlint-config-jira": "^1.5.1",
		"commitlint-plugin-jira-rules": "^1.5.1",
		"eslint": "^7.23.0",
		"husky": "^6.0.0",
		"json": "^10.0.0",
		"prettier": "^2.2.1",
		"tsc-alias": "^1.2.9",
		"typescript": "^4.2.4"
	},
	"dependencies": {}
}
